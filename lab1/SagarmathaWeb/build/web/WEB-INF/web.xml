<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Test</servlet-name>
        <servlet-class>np.edu.scst.servlet.Test</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CookiesDemo</servlet-name>
        <servlet-class>np.edu.scst.servlet.CookiesDemo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReadCookies</servlet-name>
        <servlet-class>np.edu.scst.servlet.ReadCookies</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JSPDemo</servlet-name>
        <servlet-class>np.edu.scst.servlet.JSPDemo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CookiesDemo</servlet-name>
        <url-pattern>/cookies</url-pattern>
        <url-pattern>/Cookies</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReadCookies</servlet-name>
        <url-pattern>/readcookies</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JSPDemo</servlet-name>
        <url-pattern>/jspdemo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
